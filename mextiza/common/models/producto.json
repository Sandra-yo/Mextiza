{
  "name": "Producto",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string",
      "required": true
    },
    "precio": {
      "type": "number",
      "required": true
    },
    "iva": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "productoPedido": {
      "type": "hasOne",
      "model": "ProductoPedido",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
